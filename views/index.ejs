<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link href="css/bootstrap.min.css" rel="stylesheet">
    <link href="css/font-awesome.min.css" rel="stylesheet">
  <!--  <link rel="stylesheet" href="css/templatemo_misc.css">-->
  <!-- 	<link type="text/css" rel="stylesheet" href="css/easy-responsive-tabs.css" />-->
    <link href="css/templatemo_style.css" rel="stylesheet"> 
        
	<script src="js/jquery-1.10.2.min.js"></script> 

	</head>
	<body>
		   <script>
    function showhide()
    {
        var div = document.getElementById("newpost");
		if (div.style.display !== "none") 
		{
			div.style.display = "none";
		}
		else 
		{
			div.style.display = "block";
		}
    }
    
    function getCode(ip,dname){
    	console.log(ip);
    	window.open("http://"+ip+":11101/code");
    }
    
    function saveCode(ip,dname,code){
    var timeout=setTimeout(function(){
    		alert(`${dname}:下载失败请检查网络`)
    	},5000)
    	$.post("http://"+ip+":11101/savecode",function(mes){
    		if(mes.success){
    			alert(`${dname}:下载成功运行成功`)
    			clearTimeout(timeout);
    		}
    	})
    }
    
   
  </script>
  
   <div class="logocontainer">
    	<div class="row">
        	<h1><a>设备列表</a></h1>
            <div class="clear"></div>
            <div class="templatemo_smalltitle"></div>
       </div>
    </div>
  	<div id="menu-container" class="main_menu">
  	<div class="container">
  		<%for (var i=0;i<devlist.length;i++) {  var stlye="templatemo_yellow" 
  			if((devlist[i].Name.substring(1))%4==0){
  			stlye="templatemo_yellow" 
  		}
  		else if((devlist[i].Name.substring(1))%4==1){
  			stlye="templatemo_red" 
  		}
  		else if((devlist[i].Name.substring(1))%4==2){
  			stlye="templatemo_blue" 
  		}
  		else if((devlist[i].Name.substring(1))%4==3){
  			stlye="templatemo_green" 
  		}
  		%>
  		<div class="col-md-3 col-sm-6 templatemo_leftgap">
            	<div class="<%=stlye%> templatemo_botgap">
                	<div class="templatemo_link"><a class="show-3 templatemo_page2" onclick="getCode('<%=devlist[i].IP%>','<%=devlist[i].Name%>')"><%=devlist[i].Name%></a></div>
                </div>
               
         </div>
  	<%}%>
  	
  	</div>

	</body>
</html>
